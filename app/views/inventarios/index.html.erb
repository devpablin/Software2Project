<p id="notice"><%= notice %></p>

<h1>Inventarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Tipo</th>
      <th>Precio</th>
      <th>Marca</th>
      <th>Cantidad</th>
      <th>Tipo cantidad</th>
      <th>Cod barras</th>
      <th>Fecha elaboracion</th>
      <th>Fecha vencimiento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventarios.each do |inventario| %>
      <tr>
        <td><%= inventario.nombre %></td>
        <td><%= inventario.descripcion %></td>
        <td><%= inventario.tipo %></td>
        <td><%= inventario.precio %></td>
        <td><%= inventario.marca %></td>
        <td><%= inventario.cantidad %></td>
        <td><%= inventario.tipo_cantidad %></td>
        <td><%= inventario.cod_barras %></td>
        <td><%= inventario.fecha_elaboracion %></td>
        <td><%= inventario.fecha_vencimiento %></td>
        <td><%= link_to 'Show', inventario %></td>
        <td><%= link_to 'Edit', edit_inventario_path(inventario) %></td>
        <td><%= link_to 'Destroy', inventario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventario', new_inventario_path %>
